{
    "name": "gtt/doctrine-auditable-bundle",
    "type": "symfony-bundle",
    "description": "Doctrine auditable bundle is alternative for DoctrineExtensions Loggable",
    "keywords": [
        "gtt",
        "symfony",
        "doctrine",
        "auditable",
        "loggable",
        "bundle"
    ],
    "license": "MIT",
    "require": {
        "php": "~7.2",
        "doctrine/doctrine-bundle": "~1.2",
        "doctrine/orm": "~2.3",
        "symfony/config": "~3.4 || ~4.0",
        "symfony/dependency-injection": "~3.4 || ~4.0",
        "symfony/http-kernel": "~3.4 || ~4.0",
        "symfony/property-access": "~3.4 || ~4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8",
        "symfony/security-core": "~3.4 || ~4.0"
    },
    "suggest": {
        "symfony/security-core": "For logging user made change"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Gtt\\Bundle\\DoctrineAuditableBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gtt\\Bundle\\DoctrineAuditableBundle\\": "tests"
        }
    }
}
